{
    "name": "baubyte/baubyte-framework",
    "description": "Simple Framework Inspirado en Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Baubyte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Baubyte\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --color=always --testdox -v tests/",
        "php-cs-fixer": "php-cs-fixer fix --diff"
    },
    "authors": [
        {
            "name": "BauByte",
            "email": "paredbaez.martin@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13"
    }
}
